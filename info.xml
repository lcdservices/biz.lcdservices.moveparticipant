<?xml version="1.0"?>
<extension key="biz.lcdservices.moveparticipant" type="module">
  <file>moveparticipant</file>
  <name>Move Participant</name>
  <description>Adds an action to the event tab listing allowing the user to move an event registration to a different contact record. Also adds an action to the Find Participants search result action list to move multiple participant records.</description>
  <license>AGPL-3.0</license>
  <maintainer>
    <author>Lighthouse Consulting and Design</author>
    <email>dev@lcdservices.biz</email>
  </maintainer>
  <urls>
    <url desc="Main Extension Page">https://github.com/lcdservices/biz.lcdservices.moveparticipant</url>
    <url desc="Documentation">https://github.com/lcdservices/biz.lcdservices.moveparticipant</url>
    <url desc="Support">https://github.com/lcdservices/biz.lcdservices.moveparticipant/issues</url>
    <url desc="Licensing">http://www.gnu.org/licenses/agpl-3.0.html</url>
  </urls>
  <releaseDate>2018-04-02</releaseDate>
  <version>1.0</version>
  <develStage>stable</develStage>
  <compatibility>
    <ver>4.7</ver>
  </compatibility>
  <comments></comments>
  <civix>
    <namespace>CRM/LCD/MoveParticipant</namespace>
  </civix>
</extension>
